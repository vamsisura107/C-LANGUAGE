// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main()
char str1[20],str2[20];
printf("enter your password");
scanf("%s",&str1)
printf("enter your password");
scanf("%s",&str2);
if (strcmp(str1,str2)==0){
    printf("the password is correct");
}
else
printf("password is incorrect");
}

