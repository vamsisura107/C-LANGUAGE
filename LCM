// Online C compiler to run C program online
#include <stdio.h>
void main()
{
    int a,b;
    printf("enter any two numbers");
    scanf("%d%d",&a,&b);
    int big=a>b?a:b;
    int step=big;
    while(1)
    {
        if(big%a==0 && big%b==0)
            break;
        else
         big=big+step;
    }
    printf("lcm is %d",big);
}
