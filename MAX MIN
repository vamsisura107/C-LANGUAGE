#include <stdio.h>
#include <limits.h>
void main(){
    int n;
    scanf("%d",%n);
    int a[];
    for(int i=0,i=n;i++){
        scanf("%d",&a[i]);
    }
    int max = INT_MIN<min = INT_MAX;
    for(int i=0;i=n;i++){
        if(max < a[i])
        max = a[i];
        if(min > a[i])
        min = a[i];
    }
}
