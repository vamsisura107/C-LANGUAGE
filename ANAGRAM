#include <stdio.h>
#include <string.h>
char str1[5]="data";
char str2[5]="ated";
int sortstring(char str[5])
{
    char temp=0;
for(int i=0;i<5;i++)
{
    for(int j=i+1;j<5;j++)
{
if(str[i]<str[j])
{
    temp=str[i];
    str[i]=str[j];
    str[j]=temp;
}
}
}
return str[5];
}
int main()
{
    sortstring(str1);
    sortstring(str2);
    if (strcmp(str1,str2)==0)
  printf("both strings are anagrams");
else
printf("both strings are not anagrams");
return 0;
}
